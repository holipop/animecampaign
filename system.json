{
    "id": "animecampaign",
    "title": "Epithet/Anime Campaign TTRPG",
    "description": "An unofficial fanmade system based on JelloApocalypse's Epithet/Anime Campaign system.",
    "version": 2.1,
    "authors": [
        {
            "name": "Holipop",
            "discord": "@holipop",
            "flags": {}
        }
    ],
    "compatibility": {
        "minimum": 12,
        "verified": 12.331
    },
    "flags": {
        "hotReload": {
            "extensions": ["css", "hbs", "json"],
            "paths": ["animecampaign.css", "templates", "lang"]
        }
    },
    "esmodules": [
        "animecampaign.js"
    ],
    "styles": [
        "animecampaign.css"
    ],
    "languages": [
        {
            "lang": "en",
            "name": "English",
            "path": "lang/en.json"
        }
    ],
    "packs": [
        {
            "name": "wild-magic",
            "label": "Wild Magic",
            "system": "animecampaign",
            "path": "packs/wild-magic",
            "type": "RollTable"
        },
        {
            "name": "rulebooks",
            "label": "Rulebooks",
            "system": "animecampaign",
            "path": "packs/rulebooks",
            "type": "JournalEntry"
        }
    ],
    "grid": {
        "distance": 5,
        "units": "ft"
    },
    "initiative": "1d20",
    "manifest": "https://github.com/holipop/animecampaign/releases/download/2.1/system.json",
    "download": "https://github.com/holipop/animecampaign/releases/download/2.1/animecampaign.zip",
    "primaryTokenAttribute": "stamina",
    "secondaryTokenAttribute": "proficiency",
    "documentTypes": {
        "Actor": {
            "Character": {}
        },
        "Item": {
            "Feature": {}
        }
    }
}